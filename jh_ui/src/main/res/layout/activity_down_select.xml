<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/jhd_filesbg"
    tools:context="com.joyhonest.jh_drone.drone_Select_Activity">


    <Button
        android:id="@+id/but_Back"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:background="@mipmap/jhd_back_press"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.97"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04000002" />

    <Button
        android:id="@+id/but_Filep"
        android:layout_width="110dp"
        android:layout_height="95dp"
        android:background="@mipmap/jhd_filep_off"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.52" />

    <Button
        android:id="@+id/but_Filev"
        android:layout_width="110dp"
        android:layout_height="95dp"
        android:background="@mipmap/jhd_filev_off"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.52" />

    <TextView
        android:id="@+id/Filep_Count"
        android:layout_width="35dp"
        android:gravity="center_horizontal"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:text="0"
        android:textColor="#FF000000"
        android:background="#FFFFFF"
        android:elevation="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/but_Filep"
        app:layout_constraintRight_toRightOf="@+id/but_Filep"


        tools:ignore="UnusedAttribute" />

    <TextView
        android:id="@+id/Filev_Count"
        android:layout_width="35dp"
        android:gravity="center_horizontal"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:text="0"
        android:textColor="#FF000000"
        android:background="#FFFFFF"
        android:elevation="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/but_Filev"
        app:layout_constraintRight_toRightOf="@+id/but_Filev"


        tools:ignore="UnusedAttribute" />

</androidx.constraintlayout.widget.ConstraintLayout>
